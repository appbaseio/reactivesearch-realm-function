{
	"name": "reactivesearch-realm-function",
	"version": "1.0.0",
	"description": "",
	"typings": "lib/cjs/index.d.ts",
	"main": "lib/cjs/index.js",
	"module": "lib/esm/index.js",
	"files": [
		"lib",
		"src"
	],
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"clean": "rm -rf lib",
		"start": "rollup -w --config rollup.test.config.js",
		"start:test": "jest --testTimeout=20000 --runInBand --watch",
		"ts-types": "tsc --emitDeclarationOnly --outDir lib",
		"build": "npm run clean && rollup --config rollup.config.js --environment INCLUDE_DEPS,BUILD:production",
		"test": "npm run build && jest --testTimeout=20000 --runInBand --config rollup.config.js",
		"format": "prettier --write 'src/**/*.(ts)'",
		"prepublishOnly": "npm run build",
		"realm-login": "realm-cli login",
		"realm-pull": "rm -rf realm-app && realm-cli pull --local=./realm-app",
		"realm-build": "node build",
		"realm-push": "realm-cli push --local=./realm-app",
		"dev": "nodemon --watch './**/*.ts' --exec 'ts-node --project tsconfig-dev.json' src/server.ts"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/appbaseio-confidential/reactivesearch-realm-function.git"
	},
	"keywords": [],
	"author": "Lakhan Samani",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/appbaseio-confidential/reactivesearch-realm-function/issues"
	},
	"homepage": "https://github.com/appbaseio-confidential/reactivesearch-realm-function#readme",
	"devDependencies": {
		"@rollup/plugin-node-resolve": "^13.0.4",
		"@rollup/plugin-typescript": "^8.2.5",
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.13",
		"@types/lodash.range": "^3.2.6",
		"@types/node": "^16.7.10",
		"husky": "^7.0.2",
		"jest": "^27.1.0",
		"nodemon": "^2.0.12",
		"prettier": "^2.3.2",
		"rollup": "^2.56.2",
		"rollup-plugin-filesize": "^9.1.1",
		"rollup-plugin-terser": "^7.0.2",
		"ts-node": "^10.2.0",
		"tslib": "^2.3.1",
		"typescript": "^4.3.5"
	},
	"dependencies": {
		"@types/jest": "^27.0.1",
		"cors": "^2.8.5",
		"dotenv": "^10.0.0",
		"express": "^4.17.1",
		"lodash.range": "^3.2.0",
		"mongodb": "^4.1.0",
		"ts-jest": "^27.0.5",
		"yargs": "^17.1.1"
	}
}
